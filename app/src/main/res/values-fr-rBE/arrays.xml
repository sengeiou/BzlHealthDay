<?xml version="1.0" encoding="utf-8"?>
<resources>

    <string-array name="titles">
        <item>@string/data_report_day</item>
        <item>@string/data_report_week</item>
        <item>@string/data_report_month</item>
    </string-array>

    <string-array name="WeekItems">
        <item>@string/sunday</item>
        <item>@string/saturday</item>
        <item>@string/friday</item>
        <item>@string/thursday</item>
        <item>@string/wednesday</item>
        <item>@string/tuesday</item>
        <item>@string/monday</item>
    </string-array>


    <declare-styleable name="DigitalView">
        <attr name="digiColor" format="color"/>
        <attr name="digiTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DigitalGroupView">

        <attr name="digiGroupColor" format="color"/>
        <attr name="digiGroupTextSize" format="dimension"/>
        <attr name="digiGroupFigureCounts" format="integer"/>
        <attr name="digiGroupInterval" format="dimension"/>
    </declare-styleable>
</resources>